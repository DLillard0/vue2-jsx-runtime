{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2016",
      "es2017"
    ],
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "jsxImportSource": "vue2",
    "paths": {
      "vue2/jsx-runtime": ["./lib/"],
      "vue2/jsx-dev-runtime": ["./lib/"]
    }
  }
}
